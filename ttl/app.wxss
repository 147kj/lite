/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix::after{
  clear: both;
  content: "";
  height: 0;
  display: block;
}
.fr{
  position: relative;
  float: right;
}
.fl{
  position: relative;
  float: left;
}
