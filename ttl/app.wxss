/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix::after{
  clear: both;
  content: "";
  height: 0;
  display: block;
}
.fr{
  position: relative;
  float: right;
}
.fl{
  position: relative;
  float: left;
}
.h1{
  font-size: 14px;
  color: #5b5c5c;
  width: 100px;
}
.txt12{
  font-size:12px;
  color: #888484;
}
.cnter{
  text-align: center;
  vertical-align: center;
}
